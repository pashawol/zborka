								.col-sm-7.strong 100168790
							+e.row.row
								.col-sm-5 ОКПО:
								.col-sm-7.strong 37437861
							+e.row.row
								.col-sm-5 Расчетный счет:
								.col-sm-7.strong BY73BAPB30152216300100000000 (членские взносы), BY97BAPB31352216300800000000 (спонсорский) в&nbsp;ЦБУ &#8470;&nbsp;701
							+e.row.row
								.col-sm-5 Почтовый адрес:
								.col-sm-7.strong 220030, г. Минск, ул. Герцена, 2А
							+e.row.row
								.col-sm-5 Юридический адрес:
								.col-sm-7.strong г. Минск, ул. Герцена, 2А 


				.col
