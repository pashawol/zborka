							+e.title ФОРМА ОБРАТНОЙ СВЯЗИ
							+e.sub-title  Выберите тему сообщения
						+input('text','ФИО')
						+input('tel','ТЕЛЕФОН')
						+input('email','E-MAIL ')
						+select('ЧЕМ ИНТЕРЕСУЕТЕСЬ?')
							option ЧЕМ ИНТЕРЕСУЕТЕСЬ? 1
							option ЧЕМ ИНТЕРЕСУЕТЕСЬ? 12
							option ЧЕМ ИНТЕРЕСУЕТЕСЬ? 13
						+ta('ВАШЕ СООБЩЕНИЕ')
						+polite
						+e.btn(type="submit") НАПИСАТЬ СООБЩЕНИЕ
		+e.map-wrap--page
			<script async src="https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3A427a8610f82b016a80f54b13f4fd38d8f80ed00055a1b7882dcb55014941d0ab&amp;width=100%25&amp;height=400&amp;lang=ru_RU&amp;scroll=false"></script>
	// end s-contact

mixin s-contact-06()
	// start s-contact
	+b.s-contact.section.block-with-lazy#s-contact&attributes(attributes)
		.container 
			+form-wrap(class='form-wrap--lg')
				+e.head
					+e.title.text-center Хочу стать спонсором
				.row
					.col-auto
						+in-rad('face')
							+e.STRONG.text.ttu физическо лицо
					
					.col-auto
						+in-rad('face')
							+e.STRONG.text.ttu Организация
					
				.row 
					.col-md-6
						+input('text','Название организации или ФИО')
					.col-md-6
						+select('Вид спонсорской помощи')
							option Вид спонсорской помощи 1
							option Вид спонсорской помощи 12
							option Вид спонсорской помощи 13
					.col-md-6
						+input('tel','ТЕЛЕФОН')
					.col-md-6
						+input('text','Мероприятие')
					.col-md-6
						+input('email','E-MAIL ')
					.col-md-6
						+input('text','Дата')(class="datepicker-date")
				+ta('ВАШЕ СООБЩЕНИЕ')
				+polite
				+e.btn(type="submit") НАПИСАТЬ СООБЩЕНИЕ
	// end s-contact


