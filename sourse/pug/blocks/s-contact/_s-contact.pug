						+input('tel','ТЕЛЕФОН') 
						+input('email','E-MAIL ')
						+input('text','дата рождения')(class="datepicker-date")
						.ttu.strong рейтинг
						.row
							.col
								+input('text',' TE')
							
							.col
								+input('text',' ITF')
							
							.col
								+input('text',' WTA')
							
							.col
								+input('text',' ATP')
							
							.col
								+input('text','NAT')
							
					.col-md-6
						+input('url','Укажите ссылку на видео') 
						.form-group
							+in-rad('life')
								+e.SPAN.text С проживанием
							+in-rad('life')
								+e.SPAN.text Без проживания 
						
						.form-group
							+in-rad('life2')
								+e.SPAN.text C сопровождающим
							+in-rad('life2')
								+e.SPAN.text Без сопровождающего
						+ta('ВАШЕ СООБЩЕНИЕ') 
				+polite
				+e.btn(type="submit") отправить
	// end s-contact


